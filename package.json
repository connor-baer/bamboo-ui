{
  "name": "@madebyconnor/bamboo-ui",
  "sideEffects": false,
  "version": "0.0.0-semantically-released",
  "description": "A React UI component library made by Connor",
  "main": "build/index.js",
  "module": "build/es/index.js",
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "build-storybook -c .storybook -o public",
    "create:component": "foundry run plop component",
    "lint": "foundry run eslint . --ext .js,.jsx",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --format junit -o __reports__/junit/eslint-results.xml",
    "test": "jest --watch",
    "test:unit": "jest --coverage --silent",
    "test:ci": "jest --coverage --runInBand --json --outputFile=__reports__/jest-results.json",
    "clean:js": "rimraf build",
    "build:js": "yarn clean:js && yarn build:js:cjs && yarn build:js:es",
    "build:js:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir build",
    "build:js:es": "cross-env BABEL_ENV=es babel src --out-dir build/es",
    "coverage:upload": "codecov",
    "prerelease": "yarn build:js",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connor-baer/bamboo-ui.git"
  },
  "author": "Connor Baer <hello@connorbaer.co",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connor-baer/bamboo-ui/issues"
  },
  "homepage": "https://github.com/connor-baer/bamboo-ui#readme",
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/is-prop-valid": "^0.8.2",
    "@emotion/styled": "^10.0.10",
    "downshift": "^6.0.5",
    "emotion-theming": "^10.0.10",
    "es-cookie": "^1.2.0",
    "fontfaceobserver": "^2.1.0",
    "keycode": "^2.2.0",
    "lodash": "^4.17.4",
    "nprogress": "^0.2.0",
    "open-color": "^1.6.3",
    "polished": "^4.0.3",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-docs": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-options": "^5.0.10",
    "@storybook/addon-storysource": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@sumup/foundry": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-docgen": "^4.2.1",
    "codecov": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "expect": "^26.5.3",
    "glob": "^7.1.2",
    "jest": "^26.5.3",
    "jest-axe": "^4.0.0",
    "jest-cli": "^26.5.3",
    "jest-emotion": "^10.0.10",
    "react": "^16.8.2",
    "react-docgen": "^5.3.0",
    "react-docgen-annotation-resolver": "^2.0.0",
    "react-dom": "^16.8.2",
    "react-svg-loader": "^3.0.3",
    "webpack-merge": "^5.2.0"
  },
  "peerDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8.0 < 18.0.0",
    "react-dom": ">= 16.8.0 < 18.0.0"
  }
}
