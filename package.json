{
  "name": "@madebyconnor/bamboo-ui",
  "sideEffects": false,
  "version": "0.0.0-semantically-released",
  "description": "A React UI component library made by Connor",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "test": "yarn test:lint && yarn test:unit",
    "test:ci": "yarn test:lint-js && yarn test:unit --runInBand",
    "test:lint": "foundry run eslint --quiet \"src/**/*.js\"",
    "test:lint:watch": "onchange \"src/**/*.js\" -- yarn test:lint",
    "test:lint:fix": "yarn test:lint --fix",
    "test:unit": "jest --coverage --silent",
    "test:unit:watch": "jest --watch",
    "test:unit:verbose": "jest --coverage",
    "test:unit:output": "jest --json --outputFile=jest-test-results.json --silent",
    "create:component": "foundry run plop component",
    "start": "start-storybook -p 6006",
    "prebuild": "yarn test:unit:output",
    "build:storybook": "build-storybook -c .storybook -o dist",
    "build:js": "yarn clean:js && yarn build:js:cjs && yarn build:js:es",
    "build:js:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore *.spec.js,*.story.js",
    "build:js:es": "cross-env BABEL_ENV=es babel src --out-dir lib/es --ignore *.spec.js,*.story.js",
    "clean:js": "rimraf lib",
    "predeploy": "rm -rf ./dist && mkdir dist && yarn build:storybook && cp ./src/CNAME ./dist/CNAME",
    "deploy": "gh-pages -d dist",
    "prerelease": "yarn build:js",
    "release": "semantic-release",
    "coverage:upload": "codecov",
    "prepublishOnly": "yarn build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connor-baer/bamboo-ui.git"
  },
  "author": "Connor Baer <hello@connorbaer.co",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connor-baer/bamboo-ui/issues"
  },
  "homepage": "https://github.com/connor-baer/bamboo-ui#readme",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/is-prop-valid": "^0.8.2",
    "@emotion/styled": "^10.0.10",
    "emotion-theming": "^10.0.10",
    "es-cookie": "^1.2.0",
    "fontfaceobserver": "^2.1.0",
    "keycode": "^2.2.0",
    "lodash": "^4.17.4",
    "nprogress": "^0.2.0",
    "open-color": "^1.6.3",
    "polished": "^3.4.1",
    "prop-types": "^15.6.0",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-centered": "^5.0.10",
    "@storybook/addon-info": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-options": "^5.0.10",
    "@storybook/addon-storysource": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@sumup/foundry": "^1.4.0-alpha.1",
    "@svgr/webpack": "^4.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.8",
    "@testing-library/react-hooks": "^1.1.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-docgen": "^3.0.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "expect": "^24.5.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.2",
    "jest": "^24.5.0",
    "jest-axe": "^3.1.1",
    "jest-cli": "^24.5.0",
    "jest-emotion": "^10.0.10",
    "onchange": "^5.2.0",
    "react": "^16.8.2",
    "react-docgen": "^4.1.0",
    "react-docgen-annotation-resolver": "^1.1.0",
    "react-dom": "^16.8.2",
    "react-svg-loader": "^2.1.0",
    "storybook-dark-mode": "^0.1.3",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17"
  }
}
