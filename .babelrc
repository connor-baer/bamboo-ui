{
  "env": {
    "cjs": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true
          }
        ],
        "inline-react-svg"
      ],
      "presets": [["env", { "loose": true }], "react"]
    },
    "es": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true
          }
        ],
        "inline-react-svg"
      ],
      "presets": [["env", { "loose": true, "modules": false }], "react"]
    },
    "test": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true
          }
        ],
        "inline-react-svg"
      ],
      "presets": ["env", "react"]
    },
    "static": {
      "plugins": [
        [
          "emotion",
          {
            "autoLabel": false,
            "hoist": false
          }
        ],
        "dynamic-import-node",
        [
          "module-resolver",
          {
            "root": ".",
            "alias": {
              "react-emotion": "./build/stylesheet/lib/custom-styled",
              "^.+svg$": "./build/stylesheet/SvgMock"
            },
            "extensions": [".svg", ".js"]
          }
        ],
        "inline-react-svg"
      ]
    }
  }
}
